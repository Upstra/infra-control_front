{
  "name": "infra-control_front",
  "public_name": "Upstra",
  "private": true,
  "version": "0.9.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "preview:prod": "vite preview --port 4173 --host",
    "docker:build": "docker build -t upstra-frontend .",
    "docker:run": "docker run -d -p 80:80 --name upstra-frontend upstra-frontend",
    "docker:compose": "docker-compose up -d",
    "start:pm2": "./start-pm2.sh",
    "pm2:stop": "pm2 stop upstra-frontend",
    "pm2:restart": "sudo pm2 restart upstra-frontend",
    "pm2:status": "sudo pm2 status",
    "pm2:logs": "pm2 logs upstra-frontend",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "lint": "eslint \"src/**/*.{ts,vue}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,vue,js,jsx,tsx}\""
  },
  "dependencies": {
